<% if notice.present? %>
  <div class="alert alert-info alert-dismissible" role="alert">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">Ã—</span>
    </button>
    <strong><%= notice %></strong>
  </div>
<%end%>

<div class="card">
  <div class="listview lv-bordered lv-lg">
    <div class="lv-body">
      <div class="lv-item media form">
        <%= search_form_for @q, url: browse_surveys_path(q: params[:q]) do |f| %>
        <div class="form-group">
          <div class="fg-line">
            <%= f.search_field :key_cont, class: 'form-control', placeholder: "Enter Key" %>
          </div>
        </div>
        <% end %>
        <button class="btn btn-primary btn-lg" type="submit" form="survey_search" value="Submit">Search</button>
      </div>
    </div>
  </div>
</div>
